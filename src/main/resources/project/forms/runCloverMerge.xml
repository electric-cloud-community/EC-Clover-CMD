        <required>0</required>
        <type>checkbox</type>
        <checkedValue>1</checkedValue>
        <uncheckedValue>0</uncheckedValue>
    </formElement>
    <formElement>
        <label>Additional commands for CloverMerge</label>
        <property>additionalmergecommands</property>
        <value></value>
        <documentation>Additional commands to be entered for CloverMerge</documentation>
        <required>0</required>
        <type>textarea</type>
    </formElement>
    <formElement>
        <label>Database files to merge</label>
        <property>dbfilestomerge</property>
        <documentation>Database files to merge.</documentation>
        <required>1</required>
        <type>textarea</type>
    </formElement>
